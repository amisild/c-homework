#include <iostream>
#include <vector>

using namespace std;

template <class T>
T liida(T x, T y) {
    return x+y;
}


class Punkt{
    double x;
    double y;
};

Punkt suurimElement(vector<Punkt> array){
    
    Punkt suurim = array[0];

    for (int i = 0; i < 2; i++){
        if (array[i] < array[i + 1]){
            suurim = array[i + 1];
        } else {
            suurim = array[i];
        }
    }

    return suurim;
}

int jrknr(vector<Punkt> a, Punkt nr){
    for(int i = 0; i < a.size(); i++){
        if(a[i] == nr){
            return i;
        }
    }
}

int main() {
    Punkt p1(1, 2);
    Punkt p2(3, 4);
    Punkt p3 liida<Punkt>(p1, p2);

    //punktimassiiv
    vector<Punkt> array = {p1, p2, p3};
    Punkt uusPunkt = array[1] + array[2];
    cout << "Uus punkt on " << uusPunkt << endl;
    cout << "Suurim punkt on " << suurimaElement(suurim) << endl;
    cout << "Soovitud punkti jÃ¤rjekorranumber on " << jrknr(array, p1) << endl;

    return 0;
}
