#include <iostream>
#include <vector> //std::vector

using namespace std;

class Punkt {
    protected:
    double x;
    double y;

    Punkt () = default;
    Punkt (P nx, P ny)
        : x(nx)
        , y(ny)
    {}

    Punkt() {
        cout << "Sisesta x: " << endl;
        cin >> x;
        cout << "Sisesta y: " << endl;
        cin >> y;
        cout << "Valisid x-ks " << x << " ja y-ks " << y << endl;
    }

    double saabX(){
        return x;
    }
    double saabY(){
        return y;
    }

    float lenght() {
        return x.distanceFrom(y);
    }

};

class ruumiPunkt: public Punkt{
    protected:
    double z;

    ruumiPunkt(): Punkt(){
        cout << "Sisesta z: " << endl;
        cin >> z;
    }
};

int main() {

    Punkt p1;
    p1.saabX();
    p1.saabY();

}
